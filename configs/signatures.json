[
  {
    "id": "aws_s3_unclaimed_bucket",
    "enabled": true,
    "service": "AWS S3",
    "severity": "high",
    "confidence": 90,
    "description": "S3 bucket does not exist but DNS still points to it",
    "match_logic": "AND",
    "conditions": {
      "dns": {
        "cname_contains": [
          "s3.amazonaws.com",
          ".s3.",
          "amazonaws.com"
        ]
